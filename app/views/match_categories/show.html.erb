<div class="page-header">
  <%= link_to match_categories_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_match_category_path(@match_category), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show match_category</h1>
</div>

<dl class="dl-horizontal">
  <dt>Match category:</dt>
  <dd><%= @match_category.match_category_id %></dd>

  <dt>Name:</dt>
  <dd><%= @match_category.name %></dd>

</dl>
